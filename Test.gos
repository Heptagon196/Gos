class Fib {
    var io: IO;
    var n: int;
    func __ctor() void {
        this.io.read(this.n);
    }
    func fib(n: int) int {
        if (n > 2) {
            return this.fib(n - 1) + this.fib(n - 2);
        }
        return 1;
    }
    func __call() int {
        return this.fib(this.n);
    }
}

class Test {
    var io: IO;
    func main() int {
        var fib: Fib;
        this.io.print(fib());
    }
}
